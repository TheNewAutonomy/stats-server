    "build:ssr": "ng run stats-dashboard:server:production",
    "build:moksha": "ng build --configuration=moksha",
    "build:maya": "ng build --configuration=maya",
    "build:vana": "ng build --configuration=vana",
    "build:testnet": "ng build --configuration=testnet",
    "serve": "ng serve",
    "serve:moksha": "ng serve --configuration=moksha",
    "serve:maya": "ng serve --configuration=maya",
    "serve:vana": "ng serve --configuration=vana",
    "serve:testnet": "ng serve --configuration=testnet",
